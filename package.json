{
    "name": "geo-morpher",
    "version": "0.1.0",
    "description": "Imperative GeoJSON morphing utilities with Leaflet integration",
    "type": "module",
    "main": "morphs.js",
    "exports": {
        ".": "./morphs.js",
        "./leaflet": "./src/adapters/leaflet/index.js",
        "./leaflet/morph": "./src/adapters/leaflet/morphLayers.js",
        "./leaflet/glyph": "./src/adapters/leaflet/glyphLayer.js",
        "./maplibre": "./src/adapters/maplibre/index.js",
        "./maplibre/morph": "./src/adapters/maplibre/morphLayers.js",
        "./maplibre/glyph": "./src/adapters/maplibre/glyphLayer.js",
        "./adapters/leaflet": "./src/adapters/leaflet.js",
        "./adapters/maplibre": "./src/adapters/maplibre.js",
        "./core/geomorpher": "./src/core/geomorpher.js"
    },
    "files": [
        "morphs.js",
        "src",
        "data",
        "examples",
        "README.md"
    ],
    "scripts": {
        "test": "node --test",
        "examples:browser": "npx http-server -p 4173 ."
    },
    "keywords": [
        "leaflet",
        "geojson",
        "cartogram",
        "flubber",
        "morph",
        "projection",
        "wgs84",
        "osgb",
        "coordinate-transformation",
        "gis"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@turf/turf": "^6.5.0",
        "flubber": "^0.4.2",
        "lodash": "^4.17.21",
        "maplibre-gl": "^5.8.0"
    },
    "peerDependencies": {
        "leaflet": ">=1.7.0",
        "maplibre-gl": ">=5.8.0"
    }
}